{
  "name": "ngx-page-scroll-demo",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "build:app": "ng build --prod --baseHref=/ngx-page-scroll/",
    "build:lib": "ng build ngx-page-scroll-core --prod && ng build ngx-page-scroll --prod && npm run copy:readme",
    "copy:readme": "cpx README.md dist/ngx-page-scroll && cpx README.md dist/ngx-page-scroll-core",
    "commit": "git-cz",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "prerelease:lib": "ng lint && npm run build:lib && ng test --watch false && ng e2e",
    "release:lib": "cd dist/ngx-page-scroll-core && npm publish && cd ../ngx-page-scroll && npm publish",
    "start": "ng serve",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.4",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "^9.0.4",
    "@angular/compiler": "^9.0.4",
    "@angular/core": "^9.0.4",
    "@angular/flex-layout": "~9.0.0-beta.29",
    "@angular/forms": "^9.0.4",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "^9.0.4",
    "@angular/platform-browser-dynamic": "^9.0.4",
    "@angular/router": "^9.0.4",
    "core-js": "^2.6.11",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular-devkit/build-ng-packagr": "~0.900.4",
    "@angular/cli": "^9.0.4",
    "@angular/compiler-cli": "^9.0.4",
    "@angular/language-service": "^9.0.4",
    "@commitlint/cli": "~8.3.5",
    "@commitlint/config-conventional": "~8.3.4",
    "@types/jasmine": "^3.5.7",
    "@types/jasminewd2": "^2.0.8",
    "codelyzer": "^5.2.1",
    "commitizen": "^4.0.3",
    "cpx": "^1.5.0",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.3",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-packagr": "^9.0.2",
    "protractor": "^5.4.3",
    "ts-node": "^8.6.2",
    "tslint": "~5.17.0",
    "typescript": "~3.7.5",
    "webdriver-manager": "^12.1.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
